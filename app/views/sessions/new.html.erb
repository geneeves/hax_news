<h1>Log In </h1>
  <div class="form-horizontal">
    <%= form_tag sessions_path do %>
      <%= label_tag :email %>
      <%= text_field_tag :email, params[:email] %>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
      <%= submit_tag "Log In", :class => "btn btn-success" %>
    <% end %>  
  </div> 